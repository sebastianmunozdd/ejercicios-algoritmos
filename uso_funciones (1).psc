Algoritmo uso_funciones
	definir  num, seno, coseno, tangente, rais, logartimo Como real
	
	Escribir "ingresa un numero"
	leer num
	
	seno = sen(num)
	coseno = cos(num)
	tangente = tan(num)
	rais = raiz(num)
	logaritmo = ln(num)
	
	imprimir "del numero" num
	imprimir "su seno es" seno
	imprimir "su coseno es" coseno
	imprimir "su tangente es" tangente
	imprimir "su rais es" rais
	imprimir "su logaritmo es" logaritmo
	
FinAlgoritmo



