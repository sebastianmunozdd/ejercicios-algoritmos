Algoritmo SalarioEmpleado
	
	definir numhoras como entero
	
	definir  alhora, salario como real
	
	imprimir "ingresas el numero de horas laborales"
	
	leer numhoras
	
	imprimir "ingrese del valor de la hora"
	
	leer alhora
	
	salario = numhoras * alhora
	
	imprimir "el saslario a pagar es" salario 
	 
 FinAlgoritmo
